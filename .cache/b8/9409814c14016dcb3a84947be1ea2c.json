Listener(\"submit\", (event) => {\r\n  event.preventDefault();\r\n\r\n  const data = new FormData(form);\r\n  const delay = parseInt(data.get(\"delay\"));\r\n  const step = parseInt(data.get(\"step\"));\r\n  const amount = parseInt(data.get(\"amount\"));\r\n\r\n for (let i = 0; i < amount; i++) {\r\n  createPromise(2, 1500)\r\n    .then(({ position, delay }) => {\r\n      Notiflix.Notify.success(`✅ Fulfilled promise ${position} in ${delay}ms`);\r\n    })\r\n    .catch(({ position, delay }) => {\r\n      Notiflix.Notify.failure(`❌ Rejected promise ${position} in ${delay}ms`);\r\n    });\r\n }\r\n});"},"lineCount":null}},"error":null,"hash":"c7630e78f1d4a86343681547007f1cdd","cacheData":{"env":{}}}